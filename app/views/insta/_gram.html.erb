<%= form_tag root_path, :method => 'get' do %>
       

<div class="h1">Here are the top Trending Instagram Pictures</div>


    <h2><%= submit_tag "#{@keyword}", class:"button expand" %></h2>
    
    <br />

        <% @tag.each do |tag| %>

        <%= image_tag tag.images.low_resolution.url %>
        <%= tag.caption.text %>
      <br /> Likes: <%= tag.likes.count %>
    <br />Username: <%= tag.caption.from.username %>
    <br /><a href="<%= tag.link %>"><%= tag.link %></a>

  <% end %>
  <% end %>

   <script>
      
    $(function(){
        $(".h1").typed({
        strings: [" Here are the top Trending Instagram Pictures" ],

        typeSpeed: -30
        });
    });
   
        </script>
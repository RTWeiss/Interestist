<div class="usatodayheadline">
	<h1>
		<%= @keyword.upcase %>

		<%= form_for :search do |f| %>
			<%= f.hidden_field :keyword, value: @keyword %>
			<%= f.submit %>
		<% end %>

		<%= button_to "save",
			{:controller => :news, :action => 'create'}, :method => :post %>
	</h1>
</div>
<p>
	Headlines from USA Today: </br>
	<%= @usatoday["title"] %>
	<br>
	
	<%= link_to @usatoday["description"], @usatoday["link"] %>

	<%#=@usatoday %>
</p>